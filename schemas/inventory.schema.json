{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "inventory.schema.json",
  "version": "0.9.0",
  "type": "object",
  "title": "Inventory",
  "required": [
    "VIN",
    "Modules"
  ],
  "additionalProperties": true,
  "properties": {
    "VIN": {
      "$id": "#/properties/VIN",
      "type": "string",
      "title": "VIN",
      "description": "The chassis number of the corresponding vehicle",
      "maxLength": 17,
      "minLength": 17,
      "examples": [
        "YV3R8N12191134148",
        "SUU206U11DB012358",
        "VNESFR1600M006849",
        "WEB62868713121311"
      ]
    },
    "Modules": {
      "$id": "#/properties/Modules",
      "type": "array",
      "title": "Modules",
      "uniqueItems": true,
      "items": {
        "$id": "#/properties/Modules/items",
        "type": "object",
        "$ref": "equipment.schema.json"
      }
    }
  }
}
